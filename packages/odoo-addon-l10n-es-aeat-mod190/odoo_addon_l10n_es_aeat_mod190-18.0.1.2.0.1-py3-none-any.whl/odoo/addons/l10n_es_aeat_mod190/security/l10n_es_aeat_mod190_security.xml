<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="group_aeat_mod190" model="res.groups">
        <field name="name">AEAT Model 190 (with access to sensitive data)</field>
        <field name="category_id" ref="base.module_category_hidden" />
        <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
        <field
            name="implied_ids"
            eval="[(4, ref('l10n_es_aeat.group_account_aeat'))]"
        />
    </record>
    <record id="aeat_190_company_rule" model="ir.rule">
        <field name="name">AEAT 190 multi-company</field>
        <field name="model_id" ref="model_l10n_es_aeat_mod190_report" />
        <field name="global" eval="True" />
        <field name="domain_force">[('company_id', 'in', company_ids + [False])]</field>
    </record>
</odoo>
