{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "rosbag-extract (config.json)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/rosbag_util/extract.py",
      "args": ["--config", "config.json"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "rosbag-extract (args)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/rosbag_util/extract.py",
      "args": [
        "--bags",
        "/path/to/car1.bag",
        "/path/to/car2.bag",
        "--main",
        "0",
        "--out",
        "cooperative/scene1",
        "--binary"
      ],
      "console": "integratedTerminal"
    }
  ]
}
