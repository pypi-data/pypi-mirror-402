.settings-title {
    height: auto;
    text-style: bold;
    color: $primary;
    margin-bottom: 1;
    text-align: center;
    padding: 0 0 1 0;
}

.provider-detail-row, .model-detail-row {
    width: 100%;
    height: auto;
    min-height: 1;
    padding: 0;
    margin-top: 1;
}

.provider-detail, .model-detail {
    width: 100%;
    height: auto;
    color: $text-muted;
    text-align: center;
}
