{%- for resource in resources %}
resource "{{ resource.terraform_type }}" "{{ resource.name_sanitized }}" {
  # Resource ID: {{ resource.id }}
  # Import with: terraform import {{ resource.terraform_type }}.{{ resource.name_sanitized }} {{ resource.import_id }}
  
  # Add resource-specific configuration here
  # Raw data available in: resource.raw
  
  {%- if resource.tags_formatted %}
  tags = {
    {%- for key, value in resource.tags_formatted.items() %}
    {{ key }} = "{{ value }}"
    {%- endfor %}
  }
  {%- endif %}
}
{%- endfor %}
