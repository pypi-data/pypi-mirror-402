{% extends "aws/base_resource.tf.j2" %}
{% block resources %}
{% for config in resources %}
{{ resource_spacer() }}
resource "aws_xray_encryption_config" "{{ config.name_sanitized }}" {
  type = "{{ config.type | default('NONE') }}"
  
  {% if config.get('key_id') or config.get('keyId') %}
  key_id = "{{ config.key_id | default(config.keyId) }}"
  {% endif %}
}

{% endfor %}
{% endblock %}
