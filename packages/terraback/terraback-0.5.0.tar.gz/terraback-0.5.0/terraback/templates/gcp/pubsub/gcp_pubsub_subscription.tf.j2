{% extends "gcp/base_resource.tf.j2" %}
{% block resources %}
{% for sub in resources %}
{{ resource_spacer() }}
resource "google_pubsub_subscription" "{{ sub.name_sanitized }}" {
  name    = "{{ sub.name }}"
  project = "{{ sub.project }}"
  topic   = "{{ sub.topic }}"

  lifecycle {
    prevent_destroy = true
  }
}

{% endfor %}
{% endblock %}
