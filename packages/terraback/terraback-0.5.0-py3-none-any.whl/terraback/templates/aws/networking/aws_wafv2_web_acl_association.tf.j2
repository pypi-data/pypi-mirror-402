{% extends "aws/base_resource.tf.j2" %}
{% block resources %}
{% for assoc in resources %}
{{ resource_spacer() }}
resource "aws_wafv2_web_acl_association" "{{ assoc.name_sanitized }}" {
  resource_arn = "{{ assoc.ResourceArn }}"
  web_acl_arn  = "{{ assoc.WebACLArn }}"
}

{% endfor %}
{% endblock %}
