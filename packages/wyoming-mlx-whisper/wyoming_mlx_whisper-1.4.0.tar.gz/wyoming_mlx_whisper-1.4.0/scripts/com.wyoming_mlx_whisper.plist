<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.wyoming_mlx_whisper</string>
  <key>ProgramArguments</key>
  <array>
    <string><UV-PATH></string>
    <string>tool</string>
    <string>run</string>
