    <outdated>#e6db74</outdated>
    <not_installed>#f8f8f2</not_installed>
    <error>#f92672</error>
    <header>#66d9ef</header>
    <border>#49483e</border>
    <default>#f8f8f2</default>
  </colors>
</theme>
