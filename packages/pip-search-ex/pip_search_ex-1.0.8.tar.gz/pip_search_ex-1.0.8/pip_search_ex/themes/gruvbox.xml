<theme name="gruvbox">
  <aliases>
    <alias>--theme-gruvbox</alias>
    <alias>--gruvbox</alias>
  </aliases>

  <colors>
    <installed>#b8bb26</installed>
    <outdated>#fabd2f</outdated>
    <not_installed>#ebdbb2</not_installed>
    <error>#fb4934</error>
    <header>#83a598</header>
    <border>#504945</border>
    <default>#ebdbb2</default>
  </colors>
</theme>
