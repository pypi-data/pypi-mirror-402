<theme name="catppuccin-mocha">
  <aliases>
    <alias>--theme-catppuccin-mocha</alias>
    <alias>--catppuccin-mocha</alias>
    <alias>--mocha</alias>
  </aliases>

  <colors>
    <installed>#a6e3a1</installed>
    <outdated>#f9e2af</outdated>
    <not_installed>#cdd6f4</not_installed>
    <error>#f38ba8</error>
    <header>#89b4fa</header>
    <border>#45475a</border>
    <default>#cdd6f4</default>
  </colors>
</theme>
