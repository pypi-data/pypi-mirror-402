<theme name="tokyo-night">
  <aliases>
    <alias>--theme-tokyo-night</alias>
    <alias>--tokyo-night</alias>
    <alias>--ttkn</alias>
  </aliases>

  <colors>
    <installed>#9ece6a</installed>
    <outdated>#e0af68</outdated>
    <not_installed>#c0caf5</not_installed>
    <error>#f7768e</error>
    <header>#7aa2f7</header>
    <border>#414868</border>
    <default>#c0caf5</default>
  </colors>
</theme>
