# PostgreSQL connection (with pgvector extension)
COCOINDEX_DATABASE_URL=postgresql://localhost:5432/cocode

# OpenAI API for embeddings (text-embedding-3-large)
OPENAI_API_KEY=sk-...

# Cohere API for reranking (rerank-v3.5)
COHERE_API_KEY=...

# Jina API for late chunking embeddings (optional)
# Late chunking improves retrieval by ~24% by preserving cross-chunk context
JINA_API_KEY=jina_...
# USE_LATE_CHUNKING must be true for Jina selection
# USE_LATE_CHUNKING=true

# Mistral API for Codestral Embed (optional, alternative to Jina)
# Optimized for code embeddings
MISTRAL_API_KEY=...

# Embedding provider selection: "jina", "mistral", or "openai"
# EMBEDDING_PROVIDER=jina

# Optional: embedding model override
# EMBEDDING_MODEL=text-embedding-3-large
# EMBEDDING_DIMENSIONS=1024

# Optional: provider-specific model overrides
# JINA_MODEL=jina-embeddings-v3
# MISTRAL_EMBED_MODEL=codestral-embed

# Optional: reranking model override
# RERANK_MODEL=rerank-v3.5

# Optional: Graph centrality boosting
# Prioritize structurally important files (imported by many) over peripheral files (tests, scripts)
# 1.0 = full effect (default), 0 = disabled, >1 = amplified
# CENTRALITY_WEIGHT=1.0

# Optional: Multi-hop graph traversal
# MAX_GRAPH_HOPS: Maximum hop distance for dependency traversal (default: 3 for comprehensive analysis)
# MAX_GRAPH_RESULTS: Maximum number of related files to return from graph expansion (default: 30)
# MAX_GRAPH_HOPS=3
# MAX_GRAPH_RESULTS=30

# Optional: Symbol-level indexing (functions, classes, methods)
# Enabled by default for precise code references with line numbers
# ENABLE_SYMBOL_INDEXING=true
# SYMBOL_WEIGHT=0.7  # Weight for symbol search results (vs chunk_weight=0.3)
# CHUNK_WEIGHT=0.3
