from typing import Any
from xml.dom.minidom import Document, Element

__version__: str

class XElement:
    def __init__(self, document: Document, node: Element) -> None: ...
    def __eq__(self, other: Any) -> bool: ...
    @property
    def attributes(self) -> list[str]: ...
    @property
    def children(self) -> list['XElement']: ...
    @property
    def first_attribute(self) -> str: ...
    @property
    def first_child(self) -> XElement | None: ...
    @property
    def has_attributes(self) -> bool: ...
    @property
    def has_children(self) -> bool: ...
    @property
    def last_attribute(self) -> str: ...
    @property
    def last_child(self) -> XElement | None: ...
    @property
    def level(self) -> int: ...
    @property
    def name(self) -> str: ...
    @name.setter
    def name(self, text: str) -> None: ...
    @property
    def parent(self) -> XElement | None: ...
    @property
    def value(self) -> str: ...
    @value.setter
    def value(self, text: str) -> None: ...
    def add(self, name: str, attr: str = '', value: Any = None) -> XElement: ...
    def add_comment(self, text_field: str) -> None: ...
    def add_element(self, element: XElement) -> XElement: ...
    def clone(self, deep: bool = True) -> XElement: ...
    def find_child(self, name: str) -> XElement | None: ...
    def find_descendants(self, name: str = '*') -> list['XElement']: ...
    def insert_element(self, ref_element: XElement, new_element: XElement) -> XElement: ...
    def read_attribute(self, name: str) -> str: ...
    def read_child(self, name: str, default: Any = None) -> Any: ...
    def remove(self, name: str) -> XElement | None: ...
    def remove_all(self) -> None: ...
    def remove_all_attributes(self) -> None: ...
    def remove_attribute(self, name: str) -> None: ...
    def remove_element(self, element: XElement) -> XElement | None: ...
    def replace_element(self, old_element: XElement, new_element: XElement) -> XElement: ...
    def write_attribute(self, name: str, value: Any) -> None: ...
    def write_child(self, name: str, value: Any) -> None: ...

class XDocument:
    _details: Any
    def __init__(self, filename: str, root_name: str = '', comment: str = '', details: Any = None) -> None: ...
    def _subclass_details(self) -> None: ...
    @property
    def root(self) -> XElement: ...
    def create_element(self, name: str) -> XElement: ...
    def load(self, filename: str) -> None: ...
    def save(self, filename: str = '') -> None: ...
