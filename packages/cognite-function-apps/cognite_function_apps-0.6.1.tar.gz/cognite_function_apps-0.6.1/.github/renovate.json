{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>cognitedata/renovate-config"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "pyproject.toml"
      ],
      "matchDepNames": [
        "python"
      ],
      "matchDepTypes": [
        "python"
      ],
      "enabled": false,
      "description": "Disable Python version updates - we need to support Function runtimes from 3.10 to 3.12"
    }
  ]
}
