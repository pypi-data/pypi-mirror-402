{
  "schema": {
    "additionalProperties": false,
    "properties": {
      "visits": {
        "description": "List of customer locations to be served. Maximum 3,000 deliveries allowed.",
        "items": {
          "additionalProperties": false,
          "properties": {
            "id": {
              "description": "Unique identifier for the delivery.",
              "title": "Delivery ID",
              "type": "string"
            },
            "coordinate": {
              "additionalProperties": false,
              "properties": {
                "lng": {
                  "description": "Longitude in decimal degrees.",
                  "title": "Longitude",
                  "type": "number"
                },
                "lat": {
                  "description": "Latitude in decimal degrees.",
                  "title": "Latitude",
                  "type": "number"
                }
              },
              "required": [
                "lng",
                "lat"
              ],
              "title": "Coordinate",
              "type": "object"
            },
            "order_placed_at": {
              "description": "Datetime for order placed. Use ISO 8601 format with timezone, e.g., '2025-03-16T17:40:00+09:00'",
              "format": "date-time",
              "title": "Order Placed At",
              "type": "string"
            },
            "tw_early": {
              "anyOf": [
                {
                  "format": "date-time",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Datetime for time window early. Required if time window is given. Use ISO 8601 format with timezone, e.g., '2025-03-16T17:40:00+09:00'",
              "title": "Time Window Early"
            },
            "tw_late": {
              "anyOf": [
                {
                  "format": "date-time",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Datetime for time window late. Required if time window is given. Use ISO 8601 format with timezone, e.g., '2025-03-16T17:40:00+09:00'",
              "title": "Time Window Late"
            },
            "service_duration_seconds": {
              "additionalProperties": {
                "type": "number"
              },
              "description": "Service time for each vehicle type in seconds. The time elapsed from arrival at the depot until the service completion, measured in seconds.",
              "propertyNames": {
                "enum": [
                  "bike",
                  "car",
                  "walk"
                ],
                "title": "VehicleType",
                "type": "string"
              },
              "title": "Service duration in seconds",
              "type": "object"
            },
            "volume": {
              "description": "Volume of the delivery.",
              "title": "Volume",
              "type": "number"
            },
            "weight": {
              "description": "Weight of the delivery.",
              "title": "Weight",
              "type": "number"
            },
            "unassigned_penalty": {
              "description": "Unassigned penalty of the delivery.",
              "title": "Unassigned Penalty",
              "type": "number"
            },
            "extra_demands": {
              "description": "Extra demands of the delivery.",
              "items": {
                "type": "number"
              },
              "title": "Extra Demands",
              "type": "array"
            },
            "early_delivery_required": {
              "default": false,
              "description": "Early delivery required of the delivery.",
              "title": "Early Delivery Required",
              "type": "boolean"
            },
            "early_delivery_priority": {
              "default": 1,
              "description": "Early delivery priority of the delivery.",
              "maximum": 10,
              "minimum": 1,
              "title": "Early Delivery Priority",
              "type": "integer"
            },
            "prohibited_tw_early": {
              "anyOf": [
                {
                  "format": "date-time",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Start of prohibited time window. Use ISO 8601 format with timezone.",
              "title": "Prohibited Time Window Early"
            },
            "prohibited_tw_late": {
              "anyOf": [
                {
                  "format": "date-time",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "End of prohibited time window. Use ISO 8601 format with timezone.",
              "title": "Prohibited Time Window Late"
            },
            "priority": {
              "default": 0.0,
              "description": "Priority of the delivery.",
              "minimum": 0.0,
              "title": "Priority",
              "type": "number"
            },
            "prep_time": {
              "default": 0.0,
              "description": "Preparation time for the delivery.",
              "minimum": 0.0,
              "title": "Prep Time",
              "type": "number"
            }
          },
          "required": [
            "id",
            "coordinate",
            "order_placed_at",
            "service_duration_seconds",
            "volume",
            "weight",
            "unassigned_penalty",
            "extra_demands"
          ],
          "title": "DeliveryDto",
          "type": "object"
        },
        "maxItems": 3000,
        "minItems": 1,
        "title": "Delivery List",
        "type": "array"
      },
      "vehicles": {
        "description": "Information about available vehicles for routing.",
