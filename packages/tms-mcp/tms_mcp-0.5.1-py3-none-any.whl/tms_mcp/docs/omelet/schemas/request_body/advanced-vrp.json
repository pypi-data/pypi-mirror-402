                      "additionalProperties": false,
                      "properties": {
                        "lng": {
                          "description": "Longitude in decimal degrees.",
                          "title": "Longitude",
                          "type": "number"
                        },
                        "lat": {
                          "description": "Latitude in decimal degrees.",
                          "title": "Latitude",
                          "type": "number"
                        }
                      },
                      "required": [
                        "lng",
                        "lat"
                      ],
                      "title": "Coordinate",
                      "type": "object"
                    },
                    "service_duration_seconds": {
                      "additionalProperties": {
                        "type": "number"
                      },
                      "description": "Service time for each vehicle type.",
                      "propertyNames": {
                        "enum": [
                          "bike",
                          "car",
                          "walk"
                        ],
                        "title": "VehicleType",
                        "type": "string"
                      },
                      "title": "Service duration in seconds",
                      "type": "object"
                    }
                  },
                  "required": [
                    "id",
                    "coordinate",
                    "service_duration_seconds"
                  ],
                  "title": "DepotDto",
                  "type": "object"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Depot the rider will start at.",
              "title": "Start Depot"
            },
            "end_depot": {
              "anyOf": [
                {
                  "additionalProperties": false,
                  "properties": {
                    "id": {
                      "description": "Unique identifier for the depot.",
                      "title": "Depot ID",
                      "type": "string"
                    },
                    "coordinate": {
                      "additionalProperties": false,
                      "properties": {
                        "lng": {
                          "description": "Longitude in decimal degrees.",
                          "title": "Longitude",
                          "type": "number"
                        },
                        "lat": {
                          "description": "Latitude in decimal degrees.",
                          "title": "Latitude",
                          "type": "number"
                        }
                      },
                      "required": [
                        "lng",
                        "lat"
                      ],
                      "title": "Coordinate",
                      "type": "object"
                    },
                    "service_duration_seconds": {
                      "additionalProperties": {
                        "type": "number"
                      },
                      "description": "Service time for each vehicle type.",
                      "propertyNames": {
                        "enum": [
                          "bike",
                          "car",
                          "walk"
                        ],
                        "title": "VehicleType",
                        "type": "string"
                      },
                      "title": "Service duration in seconds",
                      "type": "object"
