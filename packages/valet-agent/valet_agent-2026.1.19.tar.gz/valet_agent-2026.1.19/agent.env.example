# DeviceValet Linux Agent Configuration
# Copy to /etc/valet-agent/agent.env

# Server connection (required)
VALET_SERVER_URL=https://valet.example.com
VALET_AUTH_TOKEN=your-device-token-here

# Device identity (set during enrollment, or auto-detected)
# VALET_DEVICE_ID=
# VALET_ORGANIZATION_ID=

# Check-in interval in seconds (default: 300 = 5 minutes)
VALET_CHECKIN_INTERVAL_SECONDS=300

# Feature flags
VALET_REPORT_PACKAGES=true
VALET_REPORT_SERVICES=true
