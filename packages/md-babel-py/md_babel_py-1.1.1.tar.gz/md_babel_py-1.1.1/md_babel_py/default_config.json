{
  "evaluators": {
    "codeBlock": {
      "sh": {
        "path": "/usr/bin/env",
        "defaultArguments": ["sh"]
      },
      "figlet": {
        "path": "figlet",
        "defaultArguments": ["-f", "{font}"],
        "defaultParams": {
          "font": "standard"
        }
      },
      "python": {
        "path": "/usr/bin/env",
        "defaultArguments": ["python3", "{input_file}"],
        "inputExtension": ".py",
        "session": {
          "command": ["python3", "-m", "md_babel_py.session_server"],
          "protocol": "json"
        }
      },
      "diagon": {
        "path": "diagon",
        "defaultArguments": ["{mode}", "-style=Unicode"]
      },
      "openscad": {
        "path": "sh",
        "defaultArguments": [
          "-c",
          "xvfb-run -a openscad -o {output} --colorscheme '{colorscheme}' --imgsize {size} --projection o --viewall {input_file} && magick {output} -transparent '#1D1F21' {output}"
        ],
        "inputExtension": ".scad",
        "defaultParams": {
          "colorscheme": "Tomorrow Night",
          "size": "400,400"
        }
      },
      "node": {
        "path": "/usr/bin/env",
        "defaultArguments": ["node"],
        "session": {
          "command": ["node"]
        }
      },
      "pikchr": {
        "path": "sh",
        "defaultArguments": [
          "-c",
          "out=$(pikchr --svg-only {input_file}) && echo \"$out\" > {output} || { echo \"$out\" >&2; exit 1; }"
        ],
        "inputExtension": ".pikchr"
      },
      "asymptote": {
        "path": "sh",
        "defaultArguments": [
          "-c",
          "out=\"{output}\"; asy -f svg -o \"${out%.svg}\" {input_file}"
        ],
        "inputExtension": ".asy"
      },
      "dot": {
        "path": "dot",
        "defaultArguments": [
          "-Tsvg",
          "-o", "{output}"
        ],
        "prefix": "digraph { rankdir=LR; bgcolor=transparent; node [shape=egg height=\"0\" margin=\"0.1,0.05\" fontname=terminus fontsize=13 fontcolor=white color=\"#ffffff99\" style=\"solid\"] edge [color=\"#ffffff\" fillcolor=\"transparent\" style=\"solid\"] ",
        "suffix": "}"
      },
      "graphviz": {
        "path": "sh",
        "defaultArguments": [
          "-c",
          "dot -T{format} {args} -o {output} {input_file}"
        ],
        "inputExtension": ".dot",
        "defaultParams": {
          "format": "svg",
          "args": ""
        }
      }
    }
  }
}
