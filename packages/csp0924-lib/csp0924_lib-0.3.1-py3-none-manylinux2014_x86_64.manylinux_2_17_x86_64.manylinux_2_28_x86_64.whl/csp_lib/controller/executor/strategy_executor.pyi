from csp_lib.controller.core.command import Command as Command
from csp_lib.controller.core.context import StrategyContext as StrategyContext
from csp_lib.controller.core.execution import ExecutionMode as ExecutionMode
from csp_lib.controller.core.strategy import Strategy as Strategy
from csp_lib.core import get_logger as get_logger
from typing import Awaitable, Callable

class StrategyExecutor:
    def __init__(self, context_provider: Callable[[], StrategyContext], on_command: Callable[[Command], Awaitable[None]] | None = ...) -> None: ...
    def set_strategy(self, strategy: Strategy | None) -> None: ...
    def trigger(self) -> None: ...
    def run(self) -> None: ...
    def stop(self) -> None: ...
    def execute_once(self) -> Command: ...
    def _wait_for_execution(self, config) -> None: ...
    def _execute_strategy(self) -> Command: ...
    @property
    def last_command(self): ...
    @property
    def current_strategy(self): ...
    @property
    def is_running(self): ...
