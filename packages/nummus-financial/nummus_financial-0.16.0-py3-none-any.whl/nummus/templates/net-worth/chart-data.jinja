{% if include_oob %}
  {% with oob = True %}
    {% include "net-worth/chart-config.jinja" %}
  {% endwith %}
{% endif %}
<script>
  // prettier-ignore
  onLoad(() => {
    netWorth.update(
      JSON.parse('{{ ctx.chart | tojson }}'),
      JSON.parse('{{ ctx.accounts | tojson }}'),
      JSON.parse('{{ ctx.currency_format._asdict() | tojson }}'),
    );
  });
</script>
