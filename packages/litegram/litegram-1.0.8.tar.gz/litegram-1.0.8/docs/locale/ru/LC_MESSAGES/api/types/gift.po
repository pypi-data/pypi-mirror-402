#: ../../api/types/gift.rst:3
msgid "Gift"
msgstr ""

#: litegram.types.gift.Gift:1 of
msgid "This object represents a gift that can be sent by the bot."
msgstr ""

#: litegram.types.gift.Gift:3 of
msgid "Source: https://core.telegram.org/bots/api#gift"
msgstr ""

#: litegram.types.gift.Gift.id:1 of
msgid "Unique identifier of the gift"
msgstr ""

#: litegram.types.gift.Gift.sticker:1 of
msgid "The sticker that represents the gift"
msgstr ""

#: litegram.types.gift.Gift.star_count:1 of
msgid "The number of Telegram Stars that must be paid to send the sticker"
msgstr ""

#: litegram.types.gift.Gift.upgrade_star_count:1 of
msgid ""
"*Optional*. The number of Telegram Stars that must be paid to upgrade the"
" gift to a unique one"
msgstr ""

#: litegram.types.gift.Gift.is_premium:1 of
msgid ""
"*Optional*. :code:`True`, if the gift can only be purchased by Telegram "
"Premium subscribers"
msgstr ""

#: litegram.types.gift.Gift.has_colors:1 of
msgid ""
"*Optional*. :code:`True`, if the gift can be used (after being upgraded) "
"to customize a user's appearance"
msgstr ""

#: litegram.types.gift.Gift.total_count:1 of
msgid ""
"*Optional*. The total number of gifts of this type that can be sent by "
"all users; for limited gifts only"
msgstr ""

#: litegram.types.gift.Gift.remaining_count:1 of
msgid ""
"*Optional*. The number of remaining gifts of this type that can be sent "
"by all users; for limited gifts only"
msgstr ""

#: litegram.types.gift.Gift.personal_total_count:1 of
msgid ""
"*Optional*. The total number of gifts of this type that can be sent by "
"the bot; for limited gifts only"
msgstr ""

#: litegram.types.gift.Gift.personal_remaining_count:1 of
msgid ""
"*Optional*. The number of remaining gifts of this type that can be sent "
"by the bot; for limited gifts only"
msgstr ""

#: litegram.types.gift.Gift.background:1 of
msgid "*Optional*. Background of the gift"
msgstr ""

#: litegram.types.gift.Gift.unique_gift_variant_count:1 of
msgid ""
"*Optional*. The total number of different unique gifts that can be "
"obtained by upgrading the gift"
msgstr ""

#: litegram.types.gift.Gift.publisher_chat:1 of
msgid "*Optional*. Information about the chat that published the gift"
msgstr ""
