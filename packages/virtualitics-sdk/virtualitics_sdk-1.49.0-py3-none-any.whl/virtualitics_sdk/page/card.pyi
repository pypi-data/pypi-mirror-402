class Container(Card):
    '''
    A Container is a special type of Card that is rendered as a modal in the UI.

    :param size: The size of the modal. See :class:`~virtualitics_sdk.page.drilldown.DrilldownSize`.
    :param visible: Whether the modal is currently visible in the UI.
                    If initialized with true, the container will render open as soon as it appears on the page.
    :param on_close: A callback function to be executed when the container is closed.
                     The `default_container_on_close` helper can be used to easily create a callback
                     that sets the container\'s visibility to `False`. This is called when a user clicks the X button,
                     or clicks outside of the modal in the UI. A "Cancel" button must be added explicitly to the
                     Container\'s content.

    **EXAMPLE:**

       .. code-block:: python

           from virtualitics_sdk import (
               Button,
               ButtonType,
               Container,
               Dropdown,
               RichText,
               Row,
               StoreInterface,
               default_container_on_close,
           )


           # Define callbacks to show and hide the modal
           @page_update_callback
           async def show_modal(store_interface: StoreInterface):
               page = store_interface.get_page()
               card = page.get_card_by_id("my-modal")
               if isinstance(card, Container):
                   card.visible = True
               store_interface.update_page(page)

           @page_update_callback
           async def close_modal(store_interface: StoreInterface):
               page = store_interface.get_page()
               card = page.get_card_by_id("my-modal")
               if isinstance(card, Container):
                   card.visible = False
               store_interface.update_page(page)


           # Create a button to open the modal
           show_button = Button(title="Show Modal", on_click=show_modal, button_type=ButtonType.PAGE_UPDATE)

           # Define the content for the modal
           modal_content = [
               RichText("This is a modal."),
               Dropdown(options=["A", "B", "C"], title="My Dropdown"),
               Row([Button(title="Close", on_click=close_modal, button_type=ButtonType.PAGE_UPDATE)]),
           ]

           # Create the Container
           my_modal = Container(
               title="My Modal",
               _id="my-modal",
               content=modal_content,
               visible=False,
               on_close=default_container_on_close("my-modal"),
           )

    '''
    size: Incomplete
    visible: Incomplete
    card_type: Incomplete
    on_close: Incomplete
    has_on_close: Incomplete
    visible_changed_at: Incomplete
    def __init__(self, title: str, content: list[Element | Row], subtitle: str = '', description: str = '', _id: str | None = None, show_description: bool = True, page_update: Callable | None = None, disable_next: bool = False, updater_text: str | None = None, filters: list[InputElement] | None = None, filter_update: Callable | None = None, show_comments: bool = False, show_export: bool = False, show_share: bool = False, size: DrilldownSize = ..., visible: bool = False, on_close: PageUpdateCallback | ContainerToggleCallback | None = None) -> None: ...
    def set_visibility(self, visible: bool):
        """ Sets the visiblity of the Container. Uses force_update to account for
        cases where the front-end only container toggle was used elsewhere. """
