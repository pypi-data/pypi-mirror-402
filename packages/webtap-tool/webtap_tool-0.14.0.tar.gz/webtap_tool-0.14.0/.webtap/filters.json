{
  "groups": {
    "instagram-noise": {
      "hide": {
        "types": [
          "Script",
          "Image",
          "Other",
          "Font",
          "Stylesheet"
        ],
        "urls": [
          "*cdninstagram.com*",
          "*fbcdn.net*"
        ]
      }
    },
    "vite-dev": {
      "hide": {
        "types": [],
        "urls": [
          "*/.vite/*",
          "*/node_modules/*",
          "*.svelte",
          "*/src/lib/*"
        ]
      }
    }
  }
}