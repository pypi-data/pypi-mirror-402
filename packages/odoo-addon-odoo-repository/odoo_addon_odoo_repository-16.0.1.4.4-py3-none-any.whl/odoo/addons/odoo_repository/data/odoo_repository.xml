<odoo noupdate="1">

  <record id="odoo_repository_odoo_odoo" model="odoo.repository">
    <field name="org_id" ref="odoo_repository_org_odoo" />
    <field name="name">odoo</field>
    <field name="sequence" eval="1" />
    <field name="repo_url">https://github.com/odoo/odoo</field>
    <field name="clone_url">https://github.com/odoo/odoo</field>
    <field name="repo_type">github</field>
    <field
            name="addons_path_ids"
            eval="
      [
        (4, ref('odoo_repository.odoo_repository_addons_path_odoo_sa_base')),
        (4, ref('odoo_repository.odoo_repository_addons_path_odoo_sa_addons')),
      ]
    "
        />
    <field
            name="scan_weekday_ids"
            eval="[(4, ref('base_time_window.time_weekday_sunday'))]"
        />
  </record>

  <record id="odoo_repository_odoo_enterprise" model="odoo.repository">
    <field name="org_id" ref="odoo_repository_org_odoo" />
    <field name="name">enterprise</field>
    <field name="sequence" eval="2" />
    <field name="active" eval="False" />
    <field name="repo_url">https://github.com/odoo/enterprise</field>
    <field name="clone_url">https://github.com/odoo/enterprise</field>
    <field name="repo_type">github</field>
    <field
            name="addons_path_ids"
            eval="
      [
        (4, ref('odoo_repository.odoo_repository_addons_path_odoo_sa_enterprise')),
      ]
    "
        />
    <field
            name="scan_weekday_ids"
            eval="[(4, ref('base_time_window.time_weekday_sunday'))]"
        />
  </record>

  <record id="odoo_repository_odoo_design_themes" model="odoo.repository">
    <field name="org_id" ref="odoo_repository_org_odoo" />
    <field name="name">design-themes</field>
    <field name="sequence" eval="3" />
    <field name="repo_url">https://github.com/odoo/design-themes</field>
    <field name="clone_url">https://github.com/odoo/design-themes</field>
    <field name="repo_type">github</field>
    <field
            name="addons_path_ids"
            eval="
      [
        (4, ref('odoo_repository.odoo_repository_addons_path_odoo_sa_root')),
      ]
    "
        />
    <field
            name="scan_weekday_ids"
            eval="[(4, ref('base_time_window.time_weekday_sunday'))]"
        />
  </record>

</odoo>
