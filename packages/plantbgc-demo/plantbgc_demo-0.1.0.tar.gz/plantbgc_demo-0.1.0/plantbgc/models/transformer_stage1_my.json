{
  "type": "KerasTransformer_stage1",
  "build_params": {
    "batch_size": 4,
    "d_model": 128,
    "num_heads":4,
    "ff_dim": 512,
    "num_layers": 4,
    "dropout": 0.1,
    "freeze_encoder_ratio": 0.75,
    "freeze_score_head": true
  },
  "fit_params": {
    "timesteps": 512,
    "num_epochs": 100,
    "learning_rate": 0.0001,
    "optimizer": "adam",
    "shuffle": true,
    "validation_size": 0.1,
    "verbose": 1,
    "weighted": true,
    "init_model_path": "yuhantransformer3.pkl",
    "early_stopping": {
      "monitor": "val_auc_roc",
      "mode": "max",
      "min_delta": 0.01,
      "patience": 20
    }
  },
  "input_params": {
    "features": [
      { "type": "ProteinBorderTransformer" },
      { "type": "Pfam2VecTransformer", "vector_path": "plantbgc\\data\\pfam2vec.csv" }
    ]
  }
}
