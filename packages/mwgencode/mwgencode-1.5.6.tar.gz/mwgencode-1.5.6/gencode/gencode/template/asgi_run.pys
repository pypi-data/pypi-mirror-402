from app import create_app_swagger
from run import register_service

# Connexion creates an ASGI application (AbstractApp)
app = create_app_swagger('production')

# Keep service registration behavior consistent
register_service('production', app.app)

# uvicorn/hypercorn expect the ASGI callable named here
# Usage examples:
#   uvicorn asgi_run:app --host 0.0.0.0 --port 8899
#   hypercorn asgi_run:app -b 0.0.0.0:8899