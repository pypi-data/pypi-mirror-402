    DOWNLOADING = 'DOWNLOADING'
    TRANSFER_DONE = 'TRANSFER_DONE'
    ABORTED = 'ABORTED'
    CLOSED = 'CLOSED'
    FAILURE = 'FAILURE'

@dataclass
class FileParams:
    chmod: str
    size: int
    remote_path: PurePosixPath

