<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="import_template_wizard_form_view" model="ir.ui.view">
        <field name="name">Import Template From Redner Server</field>
        <field name="model">redner.import_template.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Template From Redner Server">
                <field name="import_template_ids" readonly="True" options="{'no_open': True}" open="False" />
                <footer>
                    <button special="cancel" data-hotkey="x" string="Cancel" type="object" class="btn-secondary" />
                </footer>
            </form>
        </field>
    </record>

    <record id="action_import_template_wizard" model="ir.actions.act_window">
        <field name="name">Import Template From Redner Server</field>
        <field name="res_model">redner.import_template.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>
