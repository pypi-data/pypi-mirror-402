{
    "operationName": "adminAddAdmin",
    "query": "mutation adminAddAdmin ( $accountId:ID! $addAdminInput:AddAdminInput!) {\n\tadmin ( accountId:$accountId ) {\n\t\taddAdmin ( input:$addAdminInput  )  {\n\t\t\tadminID\n\t\t}\n\t}\t\n}",
    "variables": {
        "addAdminInput": {
            "adminType": "LOGIN",
            "email": "string",
            "firstName": "string",
            "lastName": "string",
            "passwordNeverExpires": true,
            "updateAdminRoleInput": {
                "allowedAccounts": [
                    "id1",
                    "id2"
                ],
                "allowedEntities": {
                    "id": "id",
                    "name": "string",
                    "type": "account"
                },
                "role": {
                    "id": "id",
                    "name": "string"
                }
            }
        }
    }
}