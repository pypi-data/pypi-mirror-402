{
    "operationName": "enterpriseDirectory",
    "query": "query enterpriseDirectory ( $accountId:ID! $enterpriseDirectoryLocationListInput:EnterpriseDirectoryLocationListInput) {\n\tenterpriseDirectory ( accountId:$accountId ) {\n\t\tlocationList ( input:$enterpriseDirectoryLocationListInput  )  {\n\t\t\titems {\n\t\t\t\tid\n\t\t\t\taccount {\n\t\t\t\t\tid\n\t\t\t\t\tname\n\t\t\t\t}\n\t\t\t\ttype\n\t\t\t\tname\n\t\t\t\tbusinessUnit\n\t\t\t\tdescription\n\t\t\t\tdetails {\n\t\t\t\t\tcompanyName\n\t\t\t\t\tpostalAddress {\n\t\t\t\t\t\tstreet\n\t\t\t\t\t\tcityName\n\t\t\t\t\t\tcountry {\n\t\t\t\t\t\t\tid\n\t\t\t\t\t\t\tname\n\t\t\t\t\t\t}\n\t\t\t\t\t\tstateName\n\t\t\t\t\t\tzipCode\n\t\t\t\t\t\taddressValidated\n\t\t\t\t\t}\n\t\t\t\t\tvatId\n\t\t\t\t\tcontact {\n\t\t\t\t\t\tname\n\t\t\t\t\t\tphone\n\t\t\t\t\t\temail\n\t\t\t\t\t}\n\t\t\t\t\tshippingLocation\n\t\t\t\t}\n\t\t\t\tarchived\n\t\t\t\taudit {\n\t\t\t\t\tupdatedTime\n\t\t\t\t\tupdatedBy {\n\t\t\t\t\t\tid\n\t\t\t\t\t\tname\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t\tpageInfo {\n\t\t\t\ttotal\n\t\t\t}\n\t\t}\n\t}\t\n}",
    "variables": {
        "enterpriseDirectoryLocationListInput": {
            "locationFilterInput": {
                "account": {
                    "accountInclusion": "ALL_ACCOUNTS",
                    "in": [
                        "id1",
                        "id2"
                    ]
                },
                "countryCode": {
                    "eq": "string",
                    "in": [
                        "string1",
                        "string2"
                    ],
                    "neq": "string",
                    "nin": [
                        "string1",
                        "string2"
                    ]
                },
                "freeText": {
                    "search": "string"
                },
                "id": {
                    "eq": "id",
                    "in": [
                        "id1",
                        "id2"
                    ],
                    "neq": "id",
                    "nin": [
                        "id1",
                        "id2"
                    ]
                },
                "includeArchived": true,
                "isShippingLocation": true,
                "name": {
                    "eq": "string",
                    "in": [
                        "string1",
                        "string2"
                    ],
                    "neq": "string",
                    "nin": [
                        "string1",
                        "string2"
                    ]
                },
                "type": {
                    "eq": "BRANCH",
                    "in": "BRANCH",
                    "neq": "BRANCH",
                    "nin": "BRANCH"
                }
            },
            "locationSortInput": {
                "country": {
                    "direction": "ASC",
                    "priority": 1
                },
                "name": {
                    "direction": "ASC",
                    "priority": 1
                },
                "type": {
                    "direction": "ASC",
                    "priority": 1
                }
            },
            "pagingInput": {
                "from": 1,
                "limit": 1
            }
        }
    }
}