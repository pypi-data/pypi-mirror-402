    "variables": {
        "groupMembersListInput": {
            "groupMembersListFilterInput": {
                "name": {
                    "eq": "string",
                    "in": [
                        "string1",
                        "string2"
                    ],
                    "neq": "string",
                    "nin": [
                        "string1",
                        "string2"
                    ],
                    "regex": "string"
                },
                "type": {
