    <!-- https://purls.helmholtz-metadaten.de/emg/EMG_00000055 -->


    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/emg/EMG_00000055">
        <rdfs:subClassOf rdf:resource="https://purls.helmholtz-metadaten.de/emg/EMG_00000048"/>
        <obo:IAO_0000115>time period during which data is acquired.</obo:IAO_0000115>
        <terms:contributor>https://orcid.org/0000-0001-6534-4195</terms:contributor>
