        <obo:IAO_0000115>A physical entity which contains material intended to be investigated.</obo:IAO_0000115>
        <obo:IAO_0000119>https://biolincc.nhlbi.nih.gov/glossary/</obo:IAO_0000119>
        <obo:IAO_0000119>https://doi.org/10.1351/pac199062061193</obo:IAO_0000119>
        <obo:IAO_0000119>https://goldbook.iupac.org/terms/view/S05809</obo:IAO_0000119>
        <obo:OMO_0003012>SPEC</obo:OMO_0003012>
        <terms:contributor>https://orcid.org/0000-0001-5870-4658</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0001-6534-4195</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0001-8480-4775</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0002-0070-4337</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0002-1008-4530</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0002-1278-4928</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0002-1418-0984</terms:contributor>
        <terms:contributor>https://orcid.org/0000-0002-1507-9327</terms:contributor>
