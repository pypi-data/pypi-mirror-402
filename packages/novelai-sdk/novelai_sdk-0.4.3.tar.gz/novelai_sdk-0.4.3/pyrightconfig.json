{
  "include": [
    "src"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    ".venv",
    ".git"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportPrivateUsage": false,
  "pythonVersion": "3.13",
  "typeCheckingMode": "strict"
}

